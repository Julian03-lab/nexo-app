{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "name": "Nexo",
    "slug": "nexo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "userInterfaceStyle": "light",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3df3c5f2-b20b-4e33-b623-ff13d6e2c666"
      }
    },
    "android": {
      "package": "com.fjulian03.nexoapp",
      "adaptativeIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
